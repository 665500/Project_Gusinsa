<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.tech.project_shopping_mall.dao.LoginMapper">

		<select id="selectUserPassword" resultType="com.tech.project_shopping_mall.dto.MembersDto" parameterType="com.tech.project_shopping_mall.dto.MembersDto">
			SELECT MID,MPW,MNAME,MGENDER,MBIRTH,MPHONE,MADDR_ONE,MADDR_TWO,MADDR_THREE,MADDR_FOUR,MEMAIL,MGRADE FROM MEMBERS WHERE MID=#{mid }
		</select>
		
		<select id="findId" resultType="com.tech.project_shopping_mall.dto.MembersDto" >
			SELECT MID FROM MEMBERS WHERE MNAME=#{param1 } AND MPHONE=#{param2 }
		</select>

</mapper>