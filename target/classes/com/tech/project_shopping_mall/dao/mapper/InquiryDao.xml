<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.tech.project_shopping_mall.dao.CSDao">
		
	<insert id="Inquiry_write">
	INSERT INTO INQUIRYBOARD(INUM,ISELECT1,ITITLE,ICONTENT,IFILE,ISELECT2) 
	VALUES(INQUIRYBOARD_SEQ.NEXTVAL,#{param1},#{param2},#{param3},#{param4},#{param5})	
	</insert>
	
	<select id="InquiryMembers" resultType="com.tech.project_shopping_mall.dto.IMDto">
	SELECT INUM,FK_MID,MNAME,MPHONE,MEMAIL,
	ISELECT1,ITITLE,ICONTENT,IFILE,ISELECT2 FROM 
	(SELECT ROWNUM NUM,N.* FROM
	(SELECT * FROM INQUIRYMEMBERS ORDER BY INUM DESC) N)
	WHERE NUM BETWEEN #{param1} AND #{param2}
	</select>
	
	<select id="selectBoardTotCount" resultType="Integer">
		SELECT COUNT(*) FROM INQUIRYMEMBERS
	</select>

</mapper>